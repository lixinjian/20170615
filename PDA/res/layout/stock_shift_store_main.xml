<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.ds365.erp.wms.pda.common.headtitlebar.SubHeadTitleBar
        android:id="@+id/shiftStoreMain_headerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/shiftStoreMain_upStoreButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/red"
        android:padding="5dp"
        android:text="移库上架任务单"
        android:textColor="@color/white"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/shiftStoreMain_downStoreButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/red"
        android:padding="5dp"
        android:text="移库下架任务单"
        android:textColor="@color/white"
        android:textSize="18sp" />

</LinearLayout>