<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DateTextAttrs">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="dateformat">
            <enum name="yyyyMMddHHmmss" value="0" />
            <enum name="yyyyMMdd" value="1" />
        </attr>
        <attr name="selectTextColor" format="color" />
        <attr name="selectTextBackground" format="color" />
        <attr name="textBackground" format="color" />
    </declare-styleable>
    <declare-styleable name="dropdownAttrs">
        <attr name="displayFiled" format="string" />
        <attr name="valueFiled" format="string" />
        <attr name="valueFiledType">
            <enum name="intType" value="0" />
            <enum name="stringType" value="1" />
            <enum name="longType" value="2" />
        </attr>
        <attr name="allFlag" format="boolean"/>
    </declare-styleable>

</resources>